{
  "name": "react-auth",
  "version": "1.0.0",
  "description": "Authentication library for React.",
  "author": "Stormpath, Inc.",
  "license": "Apache-2.0",
  "files": [
    "*.md",
    "es6",
    "lib",
    "umd",
    "dist"
  ],
  "main": "lib/index",
  "jsnext:main": "es6/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/react-auth/react-auth.git"
  },
  "homepage": "https://react-auth.github.io/react-auth/",
  "bugs": "https://github.com/react-auth/react-auth/issues",
  "scripts": {
    "build": "npm run build-cjs && npm run build-es6 && npm run build-umd && npm run build-min",
    "build-cjs": "NODE_ENV=production webpack --output-library-target commonjs2 src/index.js ./lib/index.js",
    "build-es6": "NODE_ENV=production webpack --output-library-target commonjs2 src/index.js ./es6/index.js",
    "build-umd": "NODE_ENV=production webpack src/index.js umd/react-auth.js",
    "build-min": "NODE_ENV=production webpack -p src/index.js umd/react-auth.min.js",
    "build-dist": "NODE_ENV=production webpack src/index.js dist/react-auth.js && NODE_ENV=production webpack -p src/index.js dist/react-auth.min.js"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "bundle-loader": "^0.5.4",
    "css-loader": "^0.23.1",
    "eslint": "^2.9.0",
    "eslint-plugin-react": "^5.0.1",
    "fbjs": "^0.8.1",
    "flux": "^2.1.1",
    "globals": "^9.6.0",
    "gzip-size": "^3.0.0",
    "history": "^2.1.1",
    "invariant": "^2.2.1",
    "isparta-loader": "^2.0.0",
    "keymirror": "^0.1.1",
    "pretty-bytes": "^3.0.1",
    "qs": "^6.1.0",
    "react": "^15.0.2",
    "react-addons-css-transition-group": "^15.0.2",
    "react-addons-test-utils": "15.0.2",
    "react-dom": "^15.0.2",
    "react-router": "^2.4.0",
    "react-static-container": "^1.0.1",
    "react-transform-catch-errors": "^1.0.2",
    "style-loader": "^0.13.1",
    "warning": "^2.1.0",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1"
  },
  "tags": [
    "react",
    "user-management"
  ],
  "keywords": [
    "react",
    "react-auth",
    "react-component",
    "user-management"
  ]
}
